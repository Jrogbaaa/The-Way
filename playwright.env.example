# Playwright Test Environment Variables
# Copy this file to .env.local and add your test credentials

# Test user credentials for authentication
TEST_USER_EMAIL=your-test-email@example.com
TEST_USER_PASSWORD=your-test-password

# API token for authenticated requests (if your app uses them)
TEST_API_TOKEN=your-test-api-token

# Test environment settings
NODE_ENV=test
NEXT_PUBLIC_APP_URL=http://localhost:3000

# External service credentials for testing (if needed)
REPLICATE_API_TOKEN=your-replicate-token
REPLICATE_USERNAME=your-replicate-username
MODAL_TOKEN_ID=your-modal-token
MODAL_TOKEN_SECRET=your-modal-secret 